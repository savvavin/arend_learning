\data Bool
  | true
  | false

\func and (x y : Bool) : Bool
  | true, true => true
  | _, _ => false

\func eq (x y : Bool) : Bool
  | true, true => true
  | false, false => true
  | _, _ => false

\func com_and (x y : Bool) : Bool => eq (and x y) (and y x)

\func assoc_and (x y z : Bool) : Bool => eq (and x (and y z)) (and (and x y) z)
